{
    "name": "proximify/app-template",
    "description": "Template to create a component-based single page application (SPA).",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Proximify Inc",
            "email": "support@proximify.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "Proximify\\": "src"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "proximify/cli-actions": "dev-master",
        "proximify/foreign-packages": "dev-master",
        "proximify/pm": "dev-master"
    },
    "scripts": {
        "post-root-package-install": "Proximify\\AppCLI::auto",
        "post-create-project-cmd": "Proximify\\AppCLI::auto",
        "pre-install-cmd": "Proximify\\AppCLI::auto",
        "post-install-cmd": "Proximify\\AppCLI::auto",
        "pre-status-cmd": "Proximify\\AppCLI::auto",
        "post-status-cmd": "Proximify\\AppCLI::auto",
        "pre-update-cmd": "Proximify\\AppCLI::auto",
        "post-update-cmd": "Proximify\\AppCLI::auto",
        "pre-package-install": "Proximify\\AppCLI::auto",
        "post-package-install": "Proximify\\AppCLI::auto",
        "pre-package-update": "Proximify\\AppCLI::auto",
        "post-package-update": "Proximify\\AppCLI::auto",
        "pre-package-uninstall": "Proximify\\AppCLI::auto",
        "post-package-uninstall": "Proximify\\AppCLI::auto"
    },
    "require-dev": {
        "t4web/composer-lock-parser": "dev-master"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/Users/dmac/Proximify/pm",
            "options": {
                "symlink": true
            }
        }
    ]
}
